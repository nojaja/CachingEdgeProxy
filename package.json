{
  "name": "CachingEdgeProxy",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node scripts/cleanup-cache.js && jest --runInBand",
    "test:workers": "node scripts/cleanup-cache.js && jest --maxWorkers=1 --verbose",
    "test:certificates": "node scripts/cleanup-cache.js && jest src/certificates.test.js --verbose",
    "test:index": "node scripts/cleanup-cache.js && jest src/index.test.js --verbose", 
    "test:e2e": "node scripts/cleanup-cache.js && jest src/e2e/proxy.test.js --verbose",
    "test:curl": "node scripts/cleanup-cache.js && jest src/e2e/proxy.curl.test.js --verbose",
    "test:sequential": "node scripts/cleanup-cache.js && npm run test:certificates && npm run test:index && npm run test:e2e && npm run test:curl",
    "clean": "node scripts/cleanup-cache.js"
  },
  "keywords": [],
  "author": "nojaja <free.riccia@gmail.com> (https://github.com/nojaja)",
  "license": "MIT",
  "dependencies": {
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0", 
    "@playwright/test": "^1.42.0",
    "jest": "^29.7.0"
  }
}
